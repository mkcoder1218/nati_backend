{
  "name": "government-service-feedback-system",
  "version": "1.0.0",
  "description": "Backend for Government Service Feedback System",
  "main": "dist/index.js",
  "scripts": {
    "start": "node start.js",
    "start-direct": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "tsc",
    "vercel-build": "npm run build",
    "render-build": "chmod +x build.sh && ./build.sh",
    "build:production": "npm install --production=false && npm run build",
    "build:clean": "rm -rf node_modules dist && npm install && npm run build",
    "migrate": "ts-node src/db/migrate.ts",
    "migrate:run": "node run-migrations.js",
    "setup-db": "ts-node src/db/setup-db.ts",
    "setup-db-safe": "ts-node src/db/setup-db-safe.ts",
    "migrate-scheduled-reports": "ts-node src/db/run-scheduled-reports-migration.ts",
    "seed-db": "ts-node src/db/seed.ts",
    "create-admin": "ts-node src/db/create-admin.ts",
    "verify-admin": "ts-node src/db/verify-admin.ts",
    "test": "jest",
    "db:test": "node test-neon-connection.js",
    "db:import": "node import-to-neon.js",
    "db:validate": "node validate-migration.js",
    "db:test-dual": "node test-dual-database.js",
    "db:switch": "node switch-database.js",
    "db:switch-neon": "node switch-database.js neon",
    "db:switch-local": "node switch-database.js local",
    "db:switch-auto": "node switch-database.js auto",
    "db:status": "node switch-database.js status",
    "build-force": "node build-force.js"
  },
  "dependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/pdfkit": "^0.13.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^3.0.13",
    "@types/passport-oauth2": "^1.4.15",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^9.0.7",
    "axios": "^1.9.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-validator": "^7.0.1",
    "fs-extra": "^11.3.0",
    "jsonwebtoken": "^9.0.2",
    "node-pg-migrate": "^6.2.2",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-oauth2": "^1.7.0",
    "pdfkit": "^0.17.1",
    "pg": "^8.11.3",
    "pg-promise": "^11.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "ts-jest": "^29.1.1"
  }
}
